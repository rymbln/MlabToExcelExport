<Window x:Class="MlabToExcelExport.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:DataMlabToExcelExport="clr-namespace:MlabToExcelExport"

        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <DataMlabToExcelExport:SetViewModel />
    </Window.DataContext>
    <ScrollViewer>
        <StackPanel>
            <Button Click="Button_Click">GetExcel</Button>
            <DataGrid x:Name ="Grid" ItemsSource="{Binding Set}" AutoGenerateColumns="True"></DataGrid>
        </StackPanel>
    </ScrollViewer>
</Window>
